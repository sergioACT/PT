{
    "name": "policiatorreonapp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "clean-cache": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; ",
        "clean-all": "yarn clean-cache; rm yarn.lock; rm -rf ios/build; rm -rf ios/pods; rm pios/Podfile.lock; rm -rf node_modules; rm -rf node_modules;",
        "android": "npx react-native run-android --no-packager && npx react-native start --reset-cache ",
        "android-release": "npx react-native run-android --no-packager -h && npx react-native start --reset-cache ",
        "apk-android": "cd android && ./gradlew assembleRelease",
        "bundle-android": "cd android && ./gradlew bundleRelease",
        "prepare-ios": "gem install cocoapods --conservative && cd ios && pod install --repo-update",
        "ios": "yarn prepare-ios && npx react-native run-ios --simulator='iPhone 13 Pro Max' --configuration Debug --no-packager && npx react-native start --reset-cache",
        "lint": "eslint .",
        "start": "react-native start",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage"
    },
    "dependencies": {
        "@babel/plugin-transform-react-jsx": "^7.24.7",
